{
  "name": "frontend",
  "version": "1.11.0",
  "private": true,
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@tanstack/react-query": "^5.90.11",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "axios": "^1.13.2",
    "fuse.js": "^7.1.0",
    "jest": "^30.2.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router": "^7.10.0",
    "react-scripts": "^5.0.1",
    "recharts": "^3.5.1",
    "util": "^0.12.5",
    "web-vitals": "^5.1.0"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "npx jest",
    "coverage": "npx jest --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "version:patch": "npm version patch --no-git-tag-version && node scripts/sync-version.js",
    "version:minor": "npm version minor --no-git-tag-version && node scripts/sync-version.js",
    "version:major": "npm version major --no-git-tag-version && node scripts/sync-version.js",
    "sync-version": "node scripts/sync-version.js"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(axios)/)"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|webp|svg)$": "<rootDir>/tests/fileMock.ts",
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "testMatch": [
      "<rootDir>/tests/**/*.test.{js,ts,tsx}"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setupTests.ts"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json",
      "node"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "extensionsToTreatAsEsm": [],
    "resolver": null,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.test.{js,jsx,ts,tsx}",
      "!src/**/*.spec.{js,jsx,ts,tsx}",
      "!src/index.{js,ts,tsx}",
      "!src/reportWebVitals.{js,ts}",
      "!src/**/*.d.ts",
      "!src/types/**/*"
    ],
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "coverageDirectory": "coverage",
    "coverageThreshold": {
      "global": {
        "branches": 65,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@craco/craco": "^7.1.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/testing-library__react": "^10.2.0",
    "@types/testing-library__user-event": "^4.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest-environment-jsdom": "^30.2.0",
    "jsdom": "^27.2.0",
    "prettier": "^3.7.3",
    "typescript": "^5.9.3"
  }
}
